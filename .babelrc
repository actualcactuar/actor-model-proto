{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "firefox": "69",
          "chrome": "70",
          "safari": "12"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-jsx",
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "h"
      }
    ]
  ]
}
